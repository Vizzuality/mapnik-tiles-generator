<?xml version="1.0" encoding="utf-8"?>
<Map srs="+proj=longlat +ellps=WGS84 +no_defs">

  <Style name="style" filter-mode="first">
  <Rule>
    <RasterSymbolizer opacity="1" scaling="lanczos" default-mode="linear" default-color="rgba(0, 0, 0, 0)" epsilon="0.41" >
