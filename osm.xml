	<stop value="511" color="#0001ff" /> 
	<stop value="9999" color="rgba(0, 0, 0, 0)" /></RasterSymbolizer>
  </Rule>
</Style>

  <Layer name="world">
    <StyleName>style</StyleName>
    <Datasource>
